# Use sudo
superuser: "sudo "

ghost: "pacman"

# Install
install: "-S "

# Directory Commands
view directory: "ls"

change directory: "cd "

make directory: "mkdir "

change permission execute: "cmod +x"

change permission writable: "cmod +w"

# Making Files
new file: "touch "

# Extra Terminal Commands
kill: key("ctrl-c")

clear: key("ctrl-l")
